{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "indent": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off"
  }
}
